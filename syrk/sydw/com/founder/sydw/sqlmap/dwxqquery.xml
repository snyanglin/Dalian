		<isNotEmpty prepend="," property="xqmc">
				<![CDATA[ xqmc]]>
		</isNotEmpty>
		<isNotEmpty prepend="," property="xqfzr">
				<![CDATA[ xqfzr]]>
		</isNotEmpty>
		<isNotEmpty prepend="," property="wymc">
				<![CDATA[wymc]]>
		</isNotEmpty>
		<isNotEmpty prepend="," property="wylxdh">
				<![CDATA[ wylxdh]]>
		</isNotEmpty>
		,
		<include refid="insertXtzd" />
		) values (#id#
		<isNotEmpty prepend="," property="xqmc">
				<![CDATA[ #xqmc#]]>
		</isNotEmpty>
		<isNotEmpty prepend="," property="xqfzr">
